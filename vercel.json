{
    "version": 2,
    "builds": [
        {
            "src": "FILGHT/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb","runtime": "python3.10"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "FILGHT/wsgi.py"
        }
    ]
}